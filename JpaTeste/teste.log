/*C4*/SET SCHEMA PUBLIC
drop table Fornecedor if exists
drop table Produto if exists
drop table TABELA_PESSOA if exists
drop table Usuario if exists
create table Fornecedor (id bigint generated by default as identity (start with 1), cidade varchar(255), estado integer, nome varchar(255), nomeResponsavel varchar(255), telefone varchar(255), primary key (id))
create table Produto (id bigint generated by default as identity (start with 1), descricao varchar(255), nome varchar(255), qntd integer, valor double, primary key (id))
create table TABELA_PESSOA (id bigint generated by default as identity (start with 1), cidade varchar(255), cpf varchar(255) not null, dataNascimento timestamp, estado varchar(255), NOME_T varchar(255) not null, rua varchar(80), telefone varchar(255), primary key (id))
create table Usuario (id bigint generated by default as identity (start with 1), dataUltimoAcesso timestamp, login varchar(255), senha varchar(255), primary key (id))
alter table TABELA_PESSOA add constraint UK_m9lpucdppha9vidw8v822h4pg unique (cpf)
